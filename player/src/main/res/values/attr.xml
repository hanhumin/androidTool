<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerView">
        <attr name="surface_type" format="enum">
            <enum name="no_view" value="0"/>
            <enum name="surface_view" value="1"/>
            <enum name="texture_view" value="2"/>
