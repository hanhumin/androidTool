<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".service.ServiceOnRebindActivity">
    <TextView
        android:id="@+id/tv_bind_service"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_stop_service"
        android:layout_marginTop="30dp"
        android:textSize="24sp"
        android:gravity="center"
        android:background="#363"
        android:text="绑定服务"/>
    <TextView
        android:id="@+id/tv_unbind_service"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_bind_service"
        android:layout_marginTop="30dp"
        android:textSize="24sp"
        android:gravity="center"
        android:background="#363"
        android:text="解绑服务"/>

</androidx.constraintlayout.widget.ConstraintLayout>